{
    "title": "Burden of Asthma in US",
    "appLayout": {
      "mainTabs": {
        "number": 4,
        "titles": ["Costs", "QALYs", "About", "Terms"]
      },
      "subTabs": {
        "number":6,
        "titles":[{"Costs":["Map","Graph"]},
          {"QALYs":["Map","Graph"]},
          "About",
          "Terms"]
      },
      "colorScheme":"startup"

    },
    "data": {
      "classNames": ["Year", "State", "Age", "Sex"],
      "valueNames": ["qalysLost", "indirectCost", "directCost"],
      "transformNames": ["Year", "State", "Age", "Sex", "yearCombined"]

    },
    "tabs":[
      {
        "title": "Map",
        "tabType": "map",
        "inputId": "tab1",
        "mainBoxColor": "info",
        "valueBoxNumber": 3,
        "valueBoxWidths": [6,6,12],
        "tabNumber": 1,
        "layerChoices": [{
          "label":"Direct Medical Costs",
          "valueName": "directCost"
        }, {
          "label":"Indirect Costs (Productivity Loss)",
          "valueName": "indirectCost"
        }],
        "leafletMap": {
          "mapName": "mapOne",
          "basemapFile": "./static_data/canadaMap.RData",
          "palette" :[
            {
              "label":"custom",
              "value":"custom"},
            {
              "label":"custom",
              "value":"custom"
            }
            ],
          "numLayers":  2,
          "layerChoices" : [
            {
              "label":"Overall Cost",
              "value":"overall"},
            {
              "label":"Cost Per Capita",
              "value":"perCapita"

            }
            ],
          "groups" : ["Overall Cost","Cost per Capita"],
          "plotLabels" : ["Cost: $","Cost/Person: $"],
          "digits" : [-5, 1],
          "dense" : [false, true],
          "legendLabels" : ["legend1", "legend2"],
          "prefix": ["$", "$"]
        }
      },
      {
        "title" : "Graph",
        "inputId" : "tab2",
        "tabType": "graph",
        "mainBoxColor" : "info",
        "tabNumber" : 2,
        "dropdownChoices": [{
          "label":"Direct Medical Costs",
          "valueName": "directCost"
        }, {
          "label":"Indirect Cost",
          "valueName": "indirectCost"
        }],
        "dropdownSelected": ["directCost"],
        "dropdown": false,
        "pngDownloadName" : "costGraph",
        "sidebarChoicesNumber" : 7,
        "sidebarShownLabels" : ["X-Axis", "Y-Axis", "State 1", "State 2", "Year","Color",
                             "Size"],
        "columnOptions" : {"xAxis": ["Age", "Indirect Cost", "Direct Cost", "Year Combined"],
                           "yAxis": ["Age", "Indirect Cost", "Direct Cost", "Year Combined"],
                           "state1": "generate",
                           "state2": "generate",
                           "year": "generate",
                           "color":["Age", "Sex", "None"],
                           "size" : ["Indirect Cost", "Direct Cost", "None"]},
        "columnTypes" : ["x-axis", "y-axis", "State", "State", "Year", "color",
                      "size"],
        "column0":"State"
      },
      {
        "title": "Map",
        "inputId": "tab3",
        "tabType": "map",
        "mainBoxColor": "info",
        "valueBoxNumber": 1,
        "valueBoxWidths": [12],
        "tabNumber": 3,
        "layerChoices": [{
            "label":"QALYs Lost",
            "valueName": "qalysLost"
        }],
        "leafletMap": {
          "mapName": "mapThree",
          "basemapFile": "./static_data/canadaMap.RData",
          "palette" :[{
            "label":"custom",
            "value":"custom"
          },
          {
              "label": "custom",
              "value": "custom"
          }],
          "numLayers":  2,
          "layerChoices" : [
            {
                "label":"QALYs Lost",
                "value":"overall"},
            {
                "label": "QALYs Lost per Capita",
                "value": "perCapita"

            }
            ],
          "groups" : ["QALYs Lost", "QALYs Lost per Capita"],
          "plotLabels" : ["QALYs Lost: ", "QALYs Lost per Capita: "],
          "digits" : [0, 4],
          "dense" : [true, false],
          "legendLabels" : ["legend3", "legend4"],
          "prefix": ["", ""]
        }
      },
      {
        "title" : "Graph",
        "inputId" : "tab4",
        "tabType": "graph",
        "mainBoxColor" : "info",
        "tabNumber" : 4,
        "dropdownChoices": [{
          "label":"QALYs Lost",
          "valueName": "qalysLost"
        }],
        "dropdownSelected": ["qalysLost"],
        "pngDownloadName" : "qalysLostGraph",
        "sidebarChoicesNumber" : 7,
        "sidebarShownLabels" : ["X-Axis", "Y-Axis", "State 1", "State 2", "Year","Color",
                             "Size"],
        "dropdown": false,
        "columnOptions" : {"xAxis": ["Age", "QALYs Lost"],
                           "yAxis": ["Age", "QALYs Lost"],
                           "state1": "generate",
                           "state2": "generate",
                           "year": "generate",
                           "color":["Age", "Sex", "None"],
                           "size" : ["QALYs Lost", "None"]},
        "columnTypes" : ["x-axis", "y-axis", "State", "State", "Year", "color",
                      "size"],
        "column0":"State"
      },
      {
        "title": "About",
        "inputId": "tab5",
        "tabType": "text",
        "tabNumber" : 5,
        "markdownFileName" : "about.Rmd",
        "imageId" : "image1",
        "imFile" : "logos2.png"
      },
      {
        "title": "Terms",
        "inputId": "tab6",
        "tabType": "text",
        "tabNumber" : 6,
        "markdownFileName" : "disclaimer.rmd",
        "imageId" : null,
        "imFile" : null
      }
    ]
}
